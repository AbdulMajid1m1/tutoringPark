<!-- Header -->
<div id="header" class="header-bg-img">
  <header class="header">
    <div class="header-content">
      <div class="container top-container">
        <div class="row header-row">
          <div class="col-lg-8 col-xl-8">
            <div class="text-container" style="padding-top: 0 ;">
              <h1>Academics & test prep, reinvented.</h1>
              <p class="p-large" style="line-height: 33px">
                School is changing. Tests are changing. The way students learn
                is changing. Conquer these changes quickly and confidently using
                innovative, private and online tutoring for
                <span class="bold-text">British</span>,
                <span class="bold-text">American</span> and
                <span class="bold-text">IB Curriculums</span>.
              </p>
              <h6 style="color: #fafafa">
                Find a certified Tutor from just
                <strong><span class="bold-text"> $14/hour</span></strong
                >.
              </h6>
              <a class="btn-solid-lg page-scroll" href="#message">Contact Us</a>
            </div>
            <!-- end of text-container -->
          </div>
          <!-- end of col -->
          <div class="col-lg-6 col-xl-6">
            <div class="image-container">
              <div class="img-wrapper">
                <!-- <img class="img-fluid"  style="max-height:500px ;" src="images/imgs/nw-g.jpg" alt="alternative"> -->
              </div>
              <!-- end of img-wrapper -->
            </div>
            <!-- end of image-container -->
          </div>
          <!-- end of col -->
        </div>

        <%- include('private_online_tutoring')%>

        <!-- end of row -->
      </div>
      <!-- end of container -->
    </div>
    <!-- end of header-content -->
  </header>
  <!-- end of header -->
</div>

