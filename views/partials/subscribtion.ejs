<!-- Newsletter -->
<div class="form">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="text-container">
          <h2 class="news-letter-heading">News letter</h2>
          <p style="width: 100%; text-align: center">
            Stay Updated With The Latest News To Get More Qualified Leads
          </p>
          <!-- Newsletter Form -->
          <form style="position: relative;"
            data-toggle="validator"
            data-focus="false"
            action="subscribe"
            method="POST"
          >
            <div class="form-group">
              <input
                type="email"
                class="form-control-input"
                id="email"
                name="email"
                required
              />
              <label class="label-control" for="email">Email</label>
              <div class="help-block with-errors"></div>
            </div>
            <div class="form-group checkbox" style="margin-left: 30px">
              <input
                type="checkbox"
                id="nterms"
                value="Agreed-to-Terms"
                required
              />I've read and agree to TutoringPark written
              <a href="/privacy-policy">Privacy Policy</a> and
              <a href="/terms-conditions">Terms Conditions</a>
              <div class="help-block with-errors"></div>
            </div>
            <div class="form-group">
              <input
                style="bottom: -93px"
                type="submit"
                class="form-control-submit-button"
                value="SUBSCRIBE"
              />
            </div>
            <div class="form-message">
              <div id="nmsgSubmit" class="h3 text-center hidden"></div>
            </div>
          </form>
          <!-- end of newsletter form -->
        </div>
        <!-- end of text-container -->
      </div>
      <!-- end of col -->
    </div>
    <!-- end of row -->
    <!-- end of row -->
  </div>
  <!-- end of container -->
</div>
<!-- end of form -->
<!-- end of newsletter -->
